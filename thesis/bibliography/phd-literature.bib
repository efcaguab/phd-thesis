@article{lin_structural_1974,
  title = {Structural {{Controllability}}},
  author = {Lin, Ching Tal},
  date = {1974},
  journaltitle = {IEEE Transactions on Automatic Control},
  volume = {19},
  pages = {201--208},
  issn = {15582523},
  doi = {10.1109/TAC.1974.1100557},
  abstract = {The new concepts of "structure" and "structural controllability" for a linear time-invariant control system (described by a pair (texA,b/tex)) are defined and studied. The physical justification of these concepts and examples are also given. The graph of a pair (texA,b/tex) is also defined. This gives another way of describing the structure of this pair. The property of structural controllability is reduced to a property of the graph of the pair (texA,b/tex). To do this, the basic concept of a "cactus" and the related concept of a "precactus" are introduced. The main result of this paper states that the pair (texA,b/tex) is structurally controllable if an only if the graph of (texA,b/tex) is "spanned by a cactus." The result is also expressed in a more conventional way, in terms of some properties of the pair (texA,b/tex).},

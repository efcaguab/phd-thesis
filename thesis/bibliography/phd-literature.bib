@article{waser_interspecific_1978,
  title = {Interspecific Pollen Transfer and Competition between Co-Occurring Plant Species},
  author = {Waser, Nickolas M.},
  date = {1978},
  journaltitle = {Oecologia},
  volume = {36},
  pages = {223--236},
  issn = {0029-8549, 1432-1939},
  doi = {10.1007/BF00349811},
  abstract = {Computer simulations of a pollinator foraging in a mixture of two species were', used to explore how plant reproduction can be influenced by interspecific pollination movements. Interspecific pollen transfer led to strong competitive effects when availabilities of pollen, receptive stigma surfaces, or pollinator movements were limited relative to the total number of fertilizations possible in the mixed population. Results from simulations suggest that competition for pollination through interspecific pollen transfer can result in rapid exclusion of one of two species, and that such competition represents a selective force promoting stable divergence of potential competitors in habitat affinity, flowering time, or other characteristics related to pollinator sharing.},
  langid = {english},
  number = {2}
}


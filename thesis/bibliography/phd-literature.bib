@article{pires_friendship_2017,
  title = {The Friendship Paradox in Species-Rich Ecological Networks: {{Implications}} for Conservation and Monitoring},
  shorttitle = {The Friendship Paradox in Species-Rich Ecological Networks},
  author = {Pires, Mathias Mistretta and Marquitti, Flavia Maria Darcie and Guimarães, Paulo R.},
  date = {2017-05},
  journaltitle = {Biological Conservation},
  volume = {209},
  pages = {245--252},
  issn = {00063207},
  doi = {10.1016/j.biocon.2017.02.026},
  abstract = {A great challenge in ecology and conservation biology is to deal with the inherent complexity of ecological systems. Because species are embedded in species-rich systems characterized by multiple interactions, it is often hard to identify which species are really important for ecological processes such as pollination. Here we show that species-rich networks describing plant-pollinator interactions share a property with networks depicting social relationships, the friendship paradox, which allows identifying highly-connected species without detailed information on the whole network of interactions. Numerical simulations support that the identified species are those more likely to affect community structure and ecological dynamics. A sampling protocol taking into account the friendship paradox property could be adapted to field studies, helping in the search for conservation surrogates or to monitor changes in the communities, such as functional extinction or the increase in ecological importance of invasive species. We hypothesize that the friendship paradox is likely to arise in networks describing other types of ecological interactions. Besides being useful for conservation and ecosystem management, the friendship paradox may have relevant implications in other areas of biology as well.},
  langid = {english}
}

@article{pires_predicted_2016,
  title = {Predicted Rainfall Changes Disrupt Trophic Interactions in a Tropical Aquatic Ecosystem},
  author = {Pires, Aliny P. F. and Marino, Nicholas A. C. and Srivastava, Diane S. and Farjalla, Vinicius F.},
  date = {2016-10},
  journaltitle = {Ecology},
  volume = {97},
  pages = {2750--2759},
  issn = {00129658},
  doi = {10.1002/ecy.1501},
  abstract = {Changes in the distribution of rainfall and the occurrence of extreme rain events will alter the size and persistence of aquatic ecosystems. Such alterations may affect the structure of local aquatic communities in terms of species composition, and by altering species interactions. In many aquatic ecosystems, leaf litter sustains detrital food webs and could regulate the responses of communities to changes in rainfall. Few empirical studies have focused on how rainfall changes will affect aquatic communities and none have evaluated if basal resource diversity can increase resistance to such rainfall effects. In this study, we used water-­holding terrestrial bromeliads, a tropical aquatic ecosystem, to test how predicted rainfall changes and litter diversity may affect community composition and trophic interactions. We used structural equation modeling to investigate the combined effects of rainfall changes and litter diversity on trophic interactions. We demonstrated that changes in rainfall disrupted trophic relationships, even though there were only minor direct effects on species abundance, richness, and community composition. Litter diversity was not able to reduce the impact of changes in rainfall on trophic interactions. We suggest that changes in rainfall can alter the way in which species ­interact with each other, decreasing the linkages among trophic groups. Such reductions in biotic interactions under climate change will have critical consequences for the functioning of tropical aquatic ecosystems.},
  langid = {english},
  number = {10}
}

